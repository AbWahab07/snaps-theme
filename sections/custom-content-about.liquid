      {% capture img_wrapper_id %}CustomImageWrapper--{{ forloop.index }}-{{ block.settings.image_tenth.id }}{% endcapture %}
      {%- assign img_url = block.settings.image_tenth | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
      <div>
        <img id="{{ img_id }}"
             class="custom__image-slider lazyload js"
             src="{{ block.settings.image_tenth | img_url: '300x300' }}"
             data-src="{{ img_url }}"
             data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
             data-aspectratio="{{ block.settings.image_tenth.aspect_ratio }}"
             data-sizes="auto"
             alt="{{ block.settings.image_tenth.alt | escape }}">
      </div>
      {% endif %}
      {% when 'text' %}
      <div class="medium-up--text-{{ block.settings.align_text }}">
        {% if block.settings.title != blank %}
        <h4 class="h3" style="color: {{ block.settings.color_heading }}; font-size: {{ block.settings.size_heading }}px">{{ block.settings.title | escape }}</h4>
        {% endif %}
        {% if block.settings.text != blank %}
        <style>
          .custom__item--{{block.id}} .rte-setting p{
            color: {{ block.settings.color_text }};
            font-size: {{ block.settings.size_text }}px;
          }
        </style>
        <div class="rte-setting rte">{{ block.settings.text | newline_to_br }}</div>
        {% endif %}
      </div>
      {% when 'video' %}
      <div class="video-wrapper">
        {% if block.settings.video_url == blank %}
        <iframe src="//www.youtube.com/embed/_9VUPq3SxOc?rel=0&showinfo=0&vq=720" width="850" height="480" frameborder="0" allowfullscreen></iframe>
        {% else %}
        {% if block.settings.video_url.type == "youtube" %}
        <iframe src="//www.youtube.com/embed/{{ block.settings.video_url.id }}?rel=0&showinfo=0&vq=720" width="850" height="480" frameborder="0" allowfullscreen></iframe>
        {% endif %}
        {% if block.settings.video_url.type == "vimeo" %}
        <iframe src="//player.vimeo.com/video/{{ block.settings.video_url.id }}?byline=0&portrait=0&badge=0" width="850" height="480" frameborder="0" allowfullscreen></iframe>
        {% endif %}
        {% endif %}
      </div>
      {% when 'video-mp4' %}
      <div class="video-wrapper-mp4">
        {% if block.settings.video_url != blank %}
        <video autoplay playsinline loop muted style="max-width: 100%; background: rgba(0, 0, 0, 0) none repeat scroll 0% 0%; width: 100%; cursor: pointer;" src="{{ block.settings.video_url }}"></video>
        {% endif %}
      </div>
      {% when 'product' %}
      {%- assign product = all_products[block.settings.product] -%}
      {% if product.title.size > 0 %}
      {% include 'product-card-grid', max_height: max_height %}
      {% else %}
      {% comment %}
      No product yet. Show onboarding one.
      {% endcomment %}
      <div class="grid-view-item">
        <a class="grid-view-item__link" href="#">
          <div class="grid-view-item__image">
            {% capture current %}{% cycle 1, 2, 3, 4, 5, 6 %}{% endcapture %}
            {{ 'product-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
          </div>
          <div class="h4 grid-view-item__title">{{ 'homepage.onboarding.product_title' | t }}</div>
          <div class="grid-view-item__meta">
            {{ 1999 | money }}
          </div>
        </a>
      </div>
      {% endif %}
      {% when 'collection' %}
      {%- assign collection = collections[block.settings.collection] -%}
      {% include 'collection-grid-item' %}
      {% when 'html' %}
      {% if block.settings.code != blank %}
      {{ block.settings.code }}
      {% endif %}
      {% when 'empty' %}
      {% if block.settings.empty != blank %}
      {% endif %}
      {% endcase %}
    </div>
  </div>

  {% endfor %}
</div>

{% if section.blocks.size == 0 %}
{% include 'no-blocks' %}
{% endif %}



