      </div>
      {% endif %}
      {% if block.settings.image_fifth != blank %}
      {% capture img_id %}CustomImage--{{ forloop.index }}-{{ block.settings.image_fifth.id }}{% endcapture %}
      {% capture img_wrapper_id %}CustomImageWrapper--{{ forloop.index }}-{{ block.settings.image_fifth.id }}{% endcapture %}
      {%- assign img_url = block.settings.image_fifth | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
      <div>
        <img id="{{ img_id }}"
             class="custom__image-slider lazyload js"
             src="{{ block.settings.image_fifth | img_url: '300x300' }}"
             data-src="{{ img_url }}"
             data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
