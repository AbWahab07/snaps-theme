      {% capture img_id %}CustomImage--{{ forloop.index }}-{{ block.settings.image_fourth.id }}{% endcapture %}
      {% capture img_wrapper_id %}CustomImageWrapper--{{ forloop.index }}-{{ block.settings.image_fourth.id }}{% endcapture %}
      {%- assign img_url = block.settings.image_fourth | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
      <div>
        <img id="{{ img_id }}"
