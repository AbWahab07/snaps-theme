             data-src="{{ img_url }}"
             data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
             data-aspectratio="{{ block.settings.image_first.aspect_ratio }}"
             data-sizes="auto"
             alt="{{ block.settings.image_first.alt | escape }}">
      </div>
      {% endif %}
