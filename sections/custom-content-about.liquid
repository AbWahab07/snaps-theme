             data-sizes="auto"
             alt="{{ block.settings.image_fifth.alt | escape }}">
      </div>
      {% endif %}
      {% if block.settings.image_sixth != blank %}
      {% capture img_id %}CustomImage--{{ forloop.index }}-{{ block.settings.image_sixth.id }}{% endcapture %}
      {% capture img_wrapper_id %}CustomImageWrapper--{{ forloop.index }}-{{ block.settings.image_sixth.id }}{% endcapture %}
      {%- assign img_url = block.settings.image_sixth | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
